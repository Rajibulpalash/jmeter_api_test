            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.io.FileWriter;
import java.io.IOException;
FileWriter writer = null;
String location = &quot;customers.csv&quot;;
log.info(&quot;=============Started wrting Data to CSV file=======&quot;);
try {
	writer = new FileWriter(location, true);
	String custId = vars.get(&quot;id&quot;);
	String name = vars.get(&quot;name&quot;);
	String email = vars.get(&quot;email&quot;);
	String phone = vars.get(&quot;phone_number&quot;);
	writer.append(custId+&quot;,&quot;+name+&quot;,&quot;+email+&quot;,&quot;+phone_number);
	writer.append(&apos;\n&apos;);
	log.info(custId+&quot; &quot;+name+&quot; &quot;+email+“ ” +phone+” ”+phone+&quot; &quot;);	
	log.info(&quot;CSV file is created... &quot;);
	writer.flush();

} catch (IOException e) {
	e.printStackTrace();
}

writer.close();
</stringProp>
